        }
      }
    },
    "DevUpdate": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "example": "5e1debb6df2cc044ac8bf373"
        },
        "technology": {
          "type": "array",
          "items": {
            "type": "string",
            "example": "Node.js, React.js"
          }
        },
        "bio": {
          "type": "string",
          "example": "Eu amo programar em Java (mentira)"
        },
        "name": {
          "type": "string",
          "example": "Diego Fernandes"
        }
      }
    },
    "DevGet": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "example": "5e1debb6df2cc044ac8bf373"
          },
          "name": {
            "type": "string",
            "example": "Diego Fernandes"
          },
          "technology": {
            "type": "array",
            "items": {
              "type": "string",
              "example": "Node.js, React.js"
            }
          },
          "bio": {
            "type": "string",
            "example": "Eu amo programar em Java (mentira)"
          },
          "git_user": {
            "type": "string",
            "example": "diego3g"
          },
          "avatar_url": {
            "type": "string",
            "example": "https://avatars2.githubusercontent.com/u/2254731?v=4"
          },
          "location": {
            "type": "object",
            "properties": {
              "coordinates": {
                "type": "array",
                "items": {
                  "type": "number",
                  "example": "-52.4145824, -28.2724265"
                }
              },
              "_id": {
                "type": "string",
                "example": "5e1debb6df2cc044ac8bf374"
              },
              "type": {
                "type": "string",
                "example": "Point"
              }
